import React from 'react';
import logo from './logo.svg';
import './App.css';

function App() {
  let counter;

  const increment = () => {
    counter = counter + 1;
  };

  const decrement = () => {
    counter = counter - 1;
  };

  return (
    <>
      <p></p>
      <button onClick={increment}>increment</button>
      <button onClick={decrement}>Decrement</button>
    </>
  );
}

export default App;
